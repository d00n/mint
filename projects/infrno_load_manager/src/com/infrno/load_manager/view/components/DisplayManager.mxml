<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 xmlns:components="com.infrno.load_manager.view.components.*">
	
	<s:states>
		<s:State name="setup" />
		<s:State name="wizzard" />
		<s:State name="chat" />
	</s:states>
	
	<s:Panel title="Setup Wizzard" horizontalCenter="0" verticalCenter="0" includeIn="wizzard">
		<components:Wizzard id="wizzard" />
	</s:Panel>
	
	<s:HGroup width="100%" height="100%" includeIn="chat">
		<s:Scroller height="100%">
			<components:Chat height="100%" />
		</s:Scroller>
		<s:Scroller width="100%" height="100%">
			<components:WhiteBoard width="100%" height="100%" />
		</s:Scroller>
	</s:HGroup>
</s:Group>
